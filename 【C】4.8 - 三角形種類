#include <iostream>

using namespace std;

int main()
{
    int a,b,c;
    printf("請輸入三角形的三邊長:\n");
    scanf("%d%d%d",&a,&b,&c);
    
    if(a==b && b==c){
        printf("正三角形");
    }else if(a*a+b*b==c*c || a*a+c*c==b*b || b*b+c*c==a*a ){
        printf("直角三角形");
    }else if(a==b || b==c || a==c){
        printf("等腰三角形");
   
    }



    return 0;
}
